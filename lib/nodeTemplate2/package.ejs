{
  "name": "<%= name %>",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "description": "",
  "scripts": {
    "dev": "vite",
    "serve": "vite --open",
    "build": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "build:pro": "vite build --mode production",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "pnpm build:dev && vite preview",
    "lint": "vite lint",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "clean": "rmdir /s /q node_modules",
    "reinstall": "pnpm clean && pnpm install",
    "commit": "git add -A && czg && git push"
  },
  "dependencies": {
    <% for (let key in dependencies) { %>"<%= key %>": "^<%= dependencies[key] %>",<% } %>
  },
  "devDependencies": {
    <% for (let key in devDependencies) { %>"<%= key %>": "^<%= devDependencies[key] %>",<% } %>
  },
  "engines": {
    "node": "^18.18.0 || >=20.9.0",
    "pnpm": ">=9"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
